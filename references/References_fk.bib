 keywords = {ANIMALS;Bird Diseases/epidemiology/microbiology/transmission;BIRDS;Electrophoresis, Gel, Pulsed-Field;England/epidemiology;Humans;Salmonella Infections/epidemiology/transmission;Salmonella typhimurium/isolation {\&} purification;Wales/epidemiology;Zoonoses/epidemiology},
 pages = {e88968},
 volume = {9},
 number = {2},
 journal = {PLoS ONE},
 doi = {10.1371/journal.pone.0088968}
