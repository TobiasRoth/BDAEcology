@article{Hole.2002,
 abstract = {House-sparrow populations have declined sharply in Western Europe in recent decades, but the reasons for this decline have yet to be identified, despite intense public interest in the matter. Here we use a combination of field experimentation, genetic analysis and demographic data to show that a reduction in winter food supply caused by agricultural intensification is probably the principal explanation for the widespread local extinctions of rural house-sparrow populations in southern England. We show that farmland populations exhibit fine-level genetic structuring and that some populations are unable to sustain themselves (sinks), whereas others act as sources.},
