@article{Dufour.2018,
 abstract = {Huntington's disease (HD) is a fatal genetic neurological disorder caused by a mutation in the human Huntingtin (HTT) gene. This mutation confers a toxic gain of function of the encoded mutant huntingtin (mHTT) protein, leading to widespread neuropathology including the formation of mHTT-positive inclusion bodies, gene dysregulation, reduced levels of adult dentate gyrus neurogenesis and neuron loss throughout many regions of the brain. Additionally, because HTT is ubiquitously expressed, several peripheral tissues are also affected. HD patients suffer from progressive motor, cognitive, psychiatric, and metabolic symptoms, including weight loss and skeletal muscle wasting. HD patients also show neuroendocrine changes including a robust, significant elevation in circulating levels of the glucocorticoid, cortisol. Previously, we confirmed that the R6/2 mouse model of HD exhibits elevated corticosterone (the rodent homolog of cortisol) levels and demonstrated that experimentally elevated corticosterone exacerbates R6/2 HD symptomology, resulting in severe and rapid weight loss and a shorter latency to death. Given that efficacious therapeutics are lacking for HD, here we investigated whether normalizing glucocorticoid levels could serve as a viable therapeutic approach for this disease. We tested the hypothesis that normalizing glucocorticoids to wild-type levels would ameliorate HD symptomology. Wild-type (WT) and transgenic R6/2 mice were allocated to three treatment groups: 1) adrenalectomy with normalized, WT-level corticosterone replacement (10 \textgreek{m}g/ml), 2) adrenalectomy with high HD-level corticosterone replacement (35 \textgreek{m}g/ml), or 3) sham surgery with no corticosterone replacement. Normalizing corticosterone to WT levels led to an improvement in metabolic rate in male R6/2 mice, as indicated by indirect calorimetry, including a reduction in oxygen consumption and normalization of respiratory exchange ratio values (p {\textless} .05 for both). Normalizing corticosterone also ameliorated brain atrophy in female R6/2 mice and skeletal muscle wasting in both male and female R6/2 mice (p {\textless} .05 for all). Female R6/2 mice given WT-level corticosterone replacement also showed a reduction in HD neuropathological markers, including a reduction in mHTT inclusion burden in the striatum, cortex, and hippocampus (p {\textless} .05 for all). This data illustrates that ameliorating glucocorticoid dysregulation leads to a significant improvement in HD symptomology in the R6/2 mouse model and suggests that cortisol-reducing therapeutics may be of value in improving HD patient quality of life.},
 author = {Dufour, Brett D. and McBride, Jodi L.},
 year = {2018},
 title = {Normalizing glucocorticoid levels attenuates metabolic and neuropathological symptoms in the R6/2 mouse model of huntington's disease},
 pages = {214--229},
 volume = {121},
 journal = {Neurobiology of disease},
 doi = {10.1016/j.nbd.2018.09.025}
}


@article{Dulac.2010,
 abstract = {Zusammenfassung

Dulac (2010) Bilan de 3,5 ann{\'e}es de suivi de la mortalit{\'e} des chiropter{\`e}res sousl les {\'e}oliennes de Bouin (Vend{\'e}e). Symbioses 25 : 32-36.


Die untersuchten 8 WEA wurden 2003 ca. 500 m von der K{\"u}ste entfernt in einem Natura 2000-Gebiet erbaut. Die Masth{\"o}he ist 105 m, die Leistung 2.4 und 2.5 MW. Vor (ab 2000) und nach (bis 2006) dem Bau wurden die Auswirkungen der Anlagen auf V{\"o}gel untersucht. Im ersten Jahr, in dem nach toten V{\"o}geln gesucht wurden, wurden auch tote Flederm{\"a}use entdeckt. Diese wurden fortan ebenfalls systematisch erfasst. 

An jeder WEA wurde ein Quadrat von 100 x 100 m Gr{\"o}sse im Abstand von 8 m systematisch einmal pro Woche {\"u}ber das ganze Jahr abgesucht (Fig. 3). 4 Mal im Jahr (zu jeder Jahreszeit) wurden Schwundraten-Experimente sowie Sucheffiziez-Experimente durchgef{\"u}hrt. Wie genau die durchgef{\"u}hrt wurden, ist nicht erkl{\"a}rt, es ist auch keine Literaturangabe angegeben. Evt. w{\"u}rde man die detaillierten Beschreibungen im Schlussbericht des Projektes finden (Dulac 2008). Er schreibt jedoch, dass die Experimente daf{\"u}r bestimmt gewesen seien, um die Schwundrate und Auffindbarkeit von toten V{\"o}geln zu bestimmen. F{\"u}r Flederm{\"a}use h{\"a}tten sie jene Schwundraten und Sucheffizienzen verwendet, die f{\"u}r kleine V{\"o}gel (kleiner als Amseln) ermittelt worden seien.

Zus{\"a}tzlich wurde ein Fl{\"a}chenkorrekturfaktor ermittelt (es ist auch nicht ganz klar, wie dieser bestimmt wurde). Um von der Zahl gefundener Flederm{\"a}use auf die Zahl gestorbener hochzurechen, wurde die Zahl gefundener Flederm{\"a}use durch das Produkt aus Sucheffizienz, Verbleibrate und Anteil abgesuchter Fl{\"a}che geteilt. 

In den 3.5 Jahren Totfundnachsuchen wurden total 77 Flederm{\"a}use gefunden. Artenzusammensetzung ist in Tabelle 1 dargestellt. Von den 77 Flederm{\"a}usen wiesen nur 3 {\"a}usserliche Verletzungen auf (dies sein bei den V{\"o}geln ganz anders gewesen). Eine Zwergfledermaus, die in gutem Zustand war, wurde autopsiert und eine Lungenh{\"a}moragie (? h{\'e}moragie pulmonaire d'origine traumatique) festgestellt.  

91 {\%} der gefundenen Flederm{\"a}use wurden zwischen Juli und Oktober gefunden (Jahresverlauf in Fig. 5 und 6). Es wurden an allen 8 Anlagen tote Flederm{\"a}use gefunden, jedoch an 3 Anlagen im S{\"u}den des Parks deutlich h{\"a}ufiger (54{\%} der 77 Flederm{\"a}use wurden an diesen 3 Anlagen gefunden). Die Schwundratenexperimente ergaben, dass je nach Saison zwischen 25 und 67 {\%} der Kadaver innerhalb einer Woche verschwanden und 47 bis 57 {\%} der kleinen Kadaver wurden gefunden. Nach {\glqq}Anwendung der Korrektur{\grqq} (keine genaueren Angaben dar{\"u}ber, ob die Schwundraten und Sucheffizienzen {\"u}ber das Jahr gemittelt wurden oder ob pro Saison separat hochgerechnet wurde) wurden 0.9 -- 4.1 tote Flederm{\"a}use pro Woche an den 8 Anlagen gesch{\"a}tzt. Pro Jahr und (einer) Anlage wurden 6 bis 26.7 tote Flederm{\"a}use gesch{\"a}tzt. Tabelle 2: Die Zahlen sind Anzahl gestorbene Flederm{\"a}use pro Woche an den 8 Anlagen. Die Zahlen in den Klammern beziehen sich auf eine Anlage und ein Jahr.

Der hohe Anteil an Rauhautflederm{\"a}usen, die sonst im Gebiet nicht bekannt ist, zeigt, dass ziehende Arten st{\"a}rker betroffen sind als nicht-ziehende Arten. Dann folgen einige Literaturvergleiche.

Zuletzt werden einige Hypothesen augestellt, die noch zu testen sind:

Betreffend Pr{\"a}senz von Flederm{\"a}usen an WEAs:

- Weil Insekten durch WEA angezogen werden (W{\"a}rme, Licht), werden auch Flederm{\"a}use angezogen.

- Flederm{\"a}use werden durch WEA angezogen, weil sie Quartiere suchen.

- Die Rotoren erzeugen T{\"o}ne, deren Frequenz nahe bei jenen von grossen Insekten ist. Deshalb werden jagende Flederm{\"a}use angezogen.

- F{\"u}r den Park de Bouin: Wegen der K{\"u}stenn{\"a}he k{\"o}nnten ziehende Flederm{\"a}use konzentriert vorkommen.

- einfacher Zufall f{\"u}r ziehende Flederm{\"a}use

Betreffend Todesursachen:

- direkte Kollision mit dem Rotor

- direkte Kollision mit dem Masten

- Verwirblung durch Luftturbulenzen

- Barotrauma},
 author = {Dulac, Perrine},
 year = {2010},
 title = {Bilan de 3,5 ann{\'e}es de suivi de la mortalit{\'e} des chiropt{\`e}res sous les {\'e}oliennes de Bouin (Vened{\'e}e)},
 keywords = {carcass searches bat mortality France;chiroptera wind energy turbines wind turbines},
 pages = {32--36},
 volume = {25},
 journal = {Symbioses}
}


